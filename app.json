{
    "expo": {
        "name": "Pure Weight",
        "slug": "pureweight",
        "version": "0.8.2",
        "scheme": "pureweight",
        "orientation": "portrait",
        "userInterfaceStyle": "automatic",
        "icon": "./media/icons/Logo.png",
        "splash": {
            "image": "./media/icons/SplashLogo.png",
            "resizeMode": "contain",
            "backgroundColor": "#333333"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "infoPlist": {
                "UIBackgroundModes": [
                    "fetch"
                ]
            },
            "config": {
                "usesNonExemptEncryption": false
            },
            "usesAppleSignIn": false,
            "accessesContactNotes": false,
            "backgroundColor": "#2e2e2e",
            "usesIcloudStorage": false,
            "supportsTablet": true,
            "bundleIdentifier": "com.FlorianErwerth.PureWeight",
            "buildNumber": "32"
        },
        "android": {
            "permissions": [
                "VIBRATE"
            ],
            "package": "com.FlorianErwerth.PureWeight"
        },
        "plugins": [
            "expo-router",
            "expo-localization",
            "expo-screen-orientation"
        ],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "4dbee4e0-b75b-4eba-8ca6-67098ce6c3b5"
            }
        },
        "owner": "florian_erwerth",
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/4dbee4e0-b75b-4eba-8ca6-67098ce6c3b5"
        }
    }
}
